{
  "stratus": {
    "files": {
      "./": "dist/**"
    },
    "options": {
      "path": "/var/www/html/development/<%= pkg.client.string %>-<%= pkg.project.string %>/",
      "host": "<%= secret.stratus.host %>",
      "username": "<%= secret.stratus.username %>",
      "password": "<%= secret.stratus.password %>",
      "srcBasePath": "dist/",
      "createDirectories": true,
      "directoryPermissions": 509
    }
  },
  "crtv": {
    "files": {
      "./": "dist/**"
    },
    "options": {
      "path": "/var/www/html/<%= pkg.client-string %>-<%= pkg.project.string %>.crtvcontent.com/",
      "host": "<%= secret.crtv.host %>",
      "username": "<%= secret.crtv.username %>",
      "password": "<%= secret.crtv.password %>",
      "agent": "<%= secret.crtv.agent %>",
      "srcBasePath": "dist/",
      "createDirectories": true,
      "directoryPermissions": 509
    }
  }
}